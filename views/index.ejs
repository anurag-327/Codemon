<html>
    <head>
        <title>codemon -Home</title>
        <link rel="stylesheet" href="../assets/styles/output.css">
        <script src="https://kit.fontawesome.com/38b951bfd3.js" crossorigin="anonymous"></script>
    </head>
    <body class="bg-gray-100">
        <div id="loader" class=" w-[100%] h-[100vh] fixed z-50 items-center justify-center bg-black">
            <div class="m-auto mt-24">
                <img src="../assets/preloaders/giphy.gif" class="m-auto" alt="preloader">
            </div>
        </div>
        <nav class="bg-gray-200 z-10 top-0 text-black  w-full px-20 py-1 flex  justify-between  sm:flex-col">
            <div class="">
                <img class="w-9 h-9 inline sm:float-left mix-blend-color-burn" src="data:image/png;base64," alt="logo">
                <span class="inline font-extrabold sm:float-left text-2xl  mt-1 font-poppins">Codemon</span>
                <i class="fa-solid text-black fa-bars mt-3 float-right invisible sm:visible  togglenavbar "></i>
            </div>
            <div class="navbar_components_div sm:w-full mt-1 sm:hidden navbarlist">
                <ul class="flex navbar_components_ul sm:flex-col text-center gap-6   font-bold ">
                    <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/">Home</a></li>
                    <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/compiler">Compiler</a></li>
                    <% if(!user_id) {%>
                    <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/login">Register</a></li>
                    <% } %>
                    <% if(user_id) {%>
                    <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/signout">SignOut</a></li>
                    <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/profile">Profile</a></li>
                    <% } %>
                    <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/addques">Add Ques</a></li>
                    <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/testcase">Add Testcases</a></li>
                </ul>
            </div>
        </nav>

        <div class="flex flex-col gap-2 p-5 mt-20">
            <div class="flex gap-5 p-4 w-[70%] sm:w-[90%] text-center m-auto bg-gray-400 text-black rounded-md shadow-md">
                <div class="w-[40%]">
                    <a href="/?filter=all">All Questions</a>
                    
                </div>
                <div class="w-[40%]">
                    <a href="/?filter=easy">Easy Questions</a>
                    
                </div>
                <div class="w-[40%]">
                    <a href="/?filter=medium">Medium Questions</a>
                   
                </div>
                <div class="w-[40%]">
                    <a href="/?filter=hard">Hard Questions</a>
                   
                </div>
            </div>
            <div class="flex gap-5 p-4 w-[50%] text-center m-auto sm:w-[90%] bg-[#333] text-white rounded-md shadow-md">
                <div class="w-[40%]">
                    
                    <span class="whitespace-pre-wrap">Questions</span>
                </div>
               
               <div class="w-[20%]">
                <apan>Difficulty</apan> 
               </div> 
            </div>

        <% for(let i of questions) { %>
        <div class="flex gap-5 p-4 w-[50%] sm:w-[90%] m-auto bg-gray-300 text-black rounded-md shadow-md">
            <div class="w-[40%]">
                <span class="whitespace-pre-wrap"><%= i.question %></span>
            </div>
           
           <div class="w-[20%] text-center">
            <apan><%= i.difficultylevel %></apan> 
           </div>
           <div class="w-[20%]">
            <a href='/solve/<%=i._id %>' class=" underline whitespace-pre-wrap" > Solve Question</a> 
           </div>
        </div>
        <% } %>
    </div>


    <script src="../assets/scripts/index.js"></script>
    </body>
</html>