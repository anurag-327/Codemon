<html>
    <head>
        <title>codemon -Home</title>
        <link rel="stylesheet" href="../assets/styles/output.css">
        <script src="https://kit.fontawesome.com/38b951bfd3.js" crossorigin="anonymous"></script>
    </head>
    <body>
        <nav class="bg-gray-300 z-10 top-0  w-full px-20 py-1 flex shadow-lg  shadow-gray-200 justify-between  sm:flex-col">
            <div class="">
                <img class="w-9 h-9 inline sm:float-left mix-blend-color-burn" src="data:image/png;base64," alt="logo">
                <span class="inline font-extrabold sm:float-left text-2xl text-violet-600 mt-1 font-poppins">Codemon</span>
                <i class="fa-solid text-black fa-bars mt-3 float-right invisible sm:visible  togglenavbar "></i>
            </div>
            <div class="navbar_components_div sm:w-full mt-1 sm:hidden navbarlist">
                <ul class="flex navbar_components_ul sm:flex-col text-center gap-6  text-gray-800 font-bold ">
                    <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/">Home</a></li>
                    <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/compiler">Compiler</a></li>
                    <% if(!user_id) {%>
                    <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/login">Register</a></li>
                    <% } %>
                    <% if(user_id) {%>
                    <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/signout">SignOut</a></li>
                    <% } %>
                    <!-- <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/addques">Add Ques</a></li> -->
                    <!-- <li class="hover:text-blue-700 cursor-pointer hover:scale-110 transform transition duration-500"><a href="/testcase">Add Testcases</a></li> -->
                </ul>
            </div>
        </nav>
        <div class="bg-[#333] w-full h-80 flex  ">
            <div class="ml-52 mt-8 box-border w-auto h-auto flex gap-12 justify-center ">
                <div class=" w-40 h-40 border-2 rounded-md  border-white">
                    <img src="../assets/avatar/meinAvatarsmall.png" alt="avatar">
                </div>
                <div class="text-white  w-auto h-20 mt-10">
                    <div>
                        <span class="font-bold"><%= user.name %> üë®‚Äçüíª</span>
                    </div>
                    <div>
                        <span class="text-gray-300 font-extralight text-sm">Codemon Id: <%= user.username %></span>
                    </div>
                </div>
            </div>
        </div>
        <div class="flex justify-start w-[80%] sm:w-[90%] sm:flex-col m-auto">
            <div class="w-[30%] sm:w-[90%] ml-5 flex flex-col sm:flex-row gap-2 p-2">
                <div class="p-3 cursor-pointer detailsbtn sm:p-1 w-full bg-violet-400  rounded-md">
                    <span>Profile</span>
                </div>
                <div class="p-3 cursor-pointer problemssolvedbtn sm:p-1 w-full  text-black rounded-md">
                    <span>Problems Solved</span>
                </div>
                <div class="p-3 cursor-pointer sm:p-1 w-full pointsbtn  text-black rounded-md">
                    <span>Points</span>
                </div>
               
            </div>
            <div class=" w-[60%] detailscontainer  -mt-20 sm:mt-4 sm:w-[100%] sm:p-1 rounded-md z-10 p-3  m-auto bg-white shadow-xl">
                <div class="p-3">
                    <span class="font-bold">Basic Info</span>
                </div>
                <div class="flex  p-5">
                    <div class="w-[30%]">
                        <h3>Name </h3>
                    </div>
                    <div class="w-[50%]">
                        <input type="text" readonly class="outline-none placeholder:text-black rounded-sm shadow-sm p-1 bg-gray-100" placeholder="<%= user.name %>">  
                    </div>
                    <div class="">
                        <a href="#" class="underline text-blue-700">edit</a>
                     </div>
                </div>
                <hr class=" h-px bg-gray-200"> 
                <div class="flex  p-5">
                    <div class="w-[30%]">
                        <h3>Gender </h3>
                    </div>
                    <div class="w-[50%]">
                        <input type="text" readonly class="outline-none placeholder:text-black rounded-sm shadow-sm p-1 bg-gray-100" placeholder="<%= user.personaldetails.gender %>">
                    </div>
                    <div class="">
                       <a href="#" class="underline text-blue-700">edit</a>
                    </div>
                </div>
                <div class="flex  p-5">
                    <div class="w-[30%]">
                        <h3>Location </h3>
                    </div>
                    <div class="w-[50%]">
                        <input type="text" readonly class="outline-none placeholder:text-black rounded-sm shadow-sm p-1 bg-gray-100" placeholder="<%= user.personaldetails.location %>">
                    </div>
                    <div class="">
                        <a href="#" class="underline text-blue-700">edit</a>
                     </div>
                </div>
                <hr class=" h-px bg-gray-200"> 
                <div class="flex  p-5">
                    <div class="w-[30%]">
                        <h3>Summary </h3>
                    </div>
                    <div class="w-[50%]">
                        <input type="text" readonly class="outline-none placeholder:text-black rounded-sm shadow-sm p-1 bg-gray-100" placeholder="<%= user.personaldetails.summary %>">
                    </div>
                    <div class="">
                        <a href="#" class="underline text-blue-700">edit</a>
                     </div>
                </div>
                <hr class=" h-px bg-gray-200"> 
                <div class="flex  p-5">
                    <div class="w-[30%]">
                        <h3>Github </h3>
                    </div>
                    <div class="w-[50%]">
                        <input type="text" readonly class="outline-none placeholder:text-black rounded-sm shadow-sm p-1 bg-gray-100" placeholder="<%= user.socialmedia.github %>">
                    </div>
                    <div class="">
                        <a href="#" class="underline text-blue-700">edit</a>
                     </div>
                </div>
                <hr class=" h-px bg-gray-200"> 
                <div class="flex p-5">
                    <div class="w-[30%]">
                        <h3>Linkedin </h3>
                    </div>
                    <div class="w-[50%]">
                        <input type="text" readonly class="outline-none placeholder:text-black rounded-sm shadow-sm p-1 bg-gray-100" placeholder="<%= user.socialmedia.linkedin %>">
                    </div>
                    <div class="">
                        <a href="#" class="underline text-blue-700">edit</a>
                     </div>
                </div>
                <hr class=" h-px bg-gray-200"> 
                <div class="flex  p-5">
                    <div class="w-[30%]">
                        <h3>Twitter </h3>
                    </div>
                    <div class="w-[50%]">
                        <input type="text" readonly class="outline-none placeholder:text-black rounded-sm shadow-sm p-1 bg-gray-100" placeholder="<%= user.socialmedia.twitter %>">
                    </div>
                    <div class="">
                        <a href="#" class="underline text-blue-700">edit</a>
                     </div>
                </div>
                <!-- <hr class=" h-px bg-gray-200">  -->
                <div class="p-3">
                    <span class=" mt-2 font-bold">Skills</span>
                </div>
                <div class="flex  p-5">
                    <div class="w-[30%]">
                        <h3>Technical Skills </h3>
                    </div>
                    <div class="w-[50%]">
                        <input type="text" readonly class="outline-none placeholder:text-black rounded-sm shadow-sm p-1 bg-gray-100" placeholder="<%= user.personaldetails.skills %>">
                    </div>
                    <div class="">
                        <a href="#" class="underline text-blue-700">edit</a>
                     </div>
                </div> 
                    
            </div>
            <div class=" w-[60%] problemssolvedcontainer hidden -mt-20 min-h-[25rem] flex flex-col gap-4 sm:mt-4 sm:w-[100%] sm:p-1 rounded-md z-10 p-3  m-auto bg-white shadow-xl">
                <span class="font-bold">Problems Solved</span>
                <% for(let i of user.questionssolved) { %>
                   <div class="w-full p-3 flex justify-between bg-gray-500 shadow-md rounded-md text-white">
                    <div>
                        <span><%= i.questionid.question %></span>
                    </div>
                    <div>
                        <span class="font-bold"><%= i.questionid.questionpoints %> üòé</span>

                    </div>
                </div>
                    <% }%>
            </div>
        </div>
         
        <script src="../assets/scripts/account.js"></script>
    </body>
</html>